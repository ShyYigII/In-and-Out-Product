<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox fx:id="around" alignment="CENTER_LEFT" prefHeight="75.0" prefWidth="610.0" spacing="10.0" styleClass="itemBlock" stylesheets="@../../css/item.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.importBill.importBill_ItemController">
   <children>
      <ImageView fitHeight="94.0" fitWidth="124.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../img/972601.png" />
         </image>
      </ImageView>
      <Label fx:id="ImportBillName" focusTraversable="false" prefHeight="50.0" prefWidth="112.0" text="Tên">
         <HBox.margin>
            <Insets bottom="20.0" left="50.0" right="20.0" top="20.0" />
         </HBox.margin>
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="Date" focusTraversable="false" prefHeight="50.0" prefWidth="200.0" text="Label">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="seeInfoButton" mnemonicParsing="false" onAction="#click2" prefHeight="40.0" prefWidth="90.0" text="Xem">
         <styleClass>
            <String fx:value="threeButton" />
            <String fx:value="small" />
         </styleClass></Button>
      <Button fx:id="DeleteButton" focusTraversable="false" mnemonicParsing="false" onAction="#click" prefHeight="40.0" prefWidth="90.0" text="Xóa">
         <styleClass>
            <String fx:value="threeButton" />
            <String fx:value="small" />
         </styleClass></Button>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</HBox>

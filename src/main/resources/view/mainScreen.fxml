<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="50.0" xmlns="http://javafx.com/javafx/21" fx:controller="controller.mainScreen" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label text="Quản lý xuất nhập hàng ">
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Button focusTraversable="false" mnemonicParsing="false" prefHeight="60.0" prefWidth="180.0"
                text="Quản lý nhập hàng" fx:id="btnImport" onAction="#handleImportScene">
            <font>
                <Font size="13.0" />
            </font>
        </Button>
        <Button focusTraversable="false" mnemonicParsing="false" prefHeight="60.0" prefWidth="197.0" text="Xem thông tin hàng trong kho">
            <font>
                <Font size="13.0" />
            </font>
        </Button>
        <Button focusTraversable="false" mnemonicParsing="false" prefHeight="60.0" prefWidth="180.0" text="Quản lý xuất hàng" fx:id="btnExport" onAction="#handleExportScene">
            <font>
                <Font size="13.0" />
            </font>
        </Button>
    </children>
</VBox>
